  "compilerOptions": {
    "rootDir": ".",
    "jsx": "react-jsx",
    "paths": {
      "~/*": ["./app/*"]
    },
    "plugins": [
      {
        "name": "typescript-remix-routes-plugin"
      }
    ]
  }
}
