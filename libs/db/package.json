{
  "name": "@libs/db",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./ids": "./src/ids.ts"
  },
  "scripts": {
    "push": "dotenv -e .env.development -- drizzle-kit push:pg",
    "migrate.dev": "dotenv -e .env.development -- yarn migrate.up",
    "migrate.generate": "dotenv -e .env.development -- drizzle-kit generate:pg",
    "migrate.up": "tsx scripts/migrate.ts",
    "seed": "dotenv -e .env.development -- tsx scripts/seed/index.ts",
    "reset.dev": "dotenv -e .env.development -- yarn reset",
    "reset": "tsx src/do-reset.ts",
    "lint": "eslint .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@libs/settings": "workspace:*",
    "@supastack/auth-model": "workspace:*",
    "@supastack/db-model": "workspace:*",
    "@supastack/db-pg-client": "workspace:*",
    "@supastack/db-pg-migrations": "workspace:*",
    "@supastack/user-model": "workspace:*",
    "@supastack/utils-dates": "workspace:*",
    "drizzle-orm": "~0.29.3",
    "drizzle-valibot": "^0.1.1",
    "kysely": "0.27.1"
  },
  "devDependencies": {
    "@libs/editor": "workspace:*",
    "@supastack/eslint-config": "workspace:*",
    "@supastack/tsconfig": "workspace:*",
    "@supastack/utils-types": "workspace:*",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "0.20.6",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
}
